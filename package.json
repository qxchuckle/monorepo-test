{
  "name": "pnpm-workspace-test",
  "private": true,
  "scripts": {
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "lint:style": "stylelint --fix ./**/*.{css,scss,vue,html}",
    "clean:type": "rimraf ./dist",
    "type:node": "tsc -p tsconfig.node.json --noEmit --composite false",
    "type:src": "pnpm run clean:type && vue-tsc -p tsconfig.src.json --composite false --declaration --emitDeclarationOnly",
    "dts-mv": "tsx ./scripts/dts-mv.ts",
    "css-mv": "tsx ./scripts/css-mv.ts",
    "build:ui": "pnpm run lint:fix && pnpm run lint:style && pnpm run type:src && pnpm --filter ./packages/** run build && pnpm run dts-mv && pnpm run css-mv"
  },
  "devDependencies": {
    "@archoleat/prettier-define-config": "^1.0.2",
    "@eslint/js": "^9.10.0",
    "@stylistic/stylelint-plugin": "^3.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.1.3",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.78.0",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-define-config": "^1.6.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.3",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "element-plus": "^2.8.1",
    "mockjs": "^1.1.0",
    "vue": "^3.5.3",
    "vue-router": "^4.4.3"
  }
}